<!-- ======================================================================================================================-->
<!--     jobcard                                                                                                           -->
<!-- ===================================================================================================================== -->
<!ELEMENT jobcard (jobmetadata,instructions,figSection?)                   	                                     >
<!ATTLIST jobcard 
		  jobcardno    CDATA                          #IMPLIED
		  cqh-chg	(N|R|U|D)                         #IMPLIED
		  barcode	CDATA                             #IMPLIED
		  manufcard	CDATA                             #IMPLIED
		  wversion	CDATA                             #IMPLIED                                                                 >
<!-- ======================================================================================================================= -->
<!--     TASK GUEST SYSTEM DATA                                                                                              -->
<!-- ========================================================================================================================-->
<!ELEMENT jobmetadata  (model,ata,jccategory,jctype,source,operator,oemversion,sthours,
						jcversion,auth,qualv,reason,panels,miscontent,risk,role) 															  >
<!ELEMENT model                                       (#PCDATA)                                                               >
<!ELEMENT ata                                         (#PCDATA)                                                               >	
<!ELEMENT mpd                                         (#PCDATA)                                                               >
<!ELEMENT mversion                                    (#PCDATA)                                                               >	
<!ELEMENT jccategory                                  	EMPTY                                                               >
<!ATTLIST jccategory 
          category           (PART | AIRCRAFT)    	   #IMPLIED                                                           	   >
<!ELEMENT jctype                                      (#PCDATA)                                                                >
<!ATTLIST jctype 
          type           (REFERENCE|SELF|GROUP)       #IMPLIED                                                                 >		  
<!ELEMENT source                                      EMPTY                                                                >
<!ATTLIST source 
          value           (SELF|IPC|AMM)    		   "AMM"                                                                   >
<!ELEMENT oemversion                                  (#PCDATA)                                                                >
<!ELEMENT jcversion                                   (#PCDATA)                                                                >
<!ELEMENT auth										  (#PCDATA) 															   >
<!ELEMENT qualv                         			  (#PCDATA)                                                                >
<!ELEMENT reason                                      (#PCDATA)                                                                >		  
<!ELEMENT miscontent (jcno?,jcversion?,model?,acno?,ata?,applic,title?,jobpart?,threshold?,interval?,skill?,
					  feedback?,mpd?,mshours?,msno?,rii?,role?,accomplished?) >
<!ELEMENT jcno                                      (#PCDATA)                                                                  >
<!ELEMENT acno                                      (#PCDATA)                                                                  >
<!ELEMENT applic                                    (#PCDATA)                                                                  >
<!ELEMENT jobpart   (partname,pn,sn,position,fin,reference)                                                                    >
<!ELEMENT partname                                  (#PCDATA)                                                                  >
<!ELEMENT sn                                        (#PCDATA)                                                                  >
<!ELEMENT position                                  (#PCDATA)                                                                  >
<!ELEMENT fin                                       (#PCDATA)                                                                  >
<!ELEMENT reference                                 (#PCDATA)                                                                  >
<!ELEMENT threshold     (limit*)       																		   				   >
<!ELEMENT interval      (limit*)      										                                                   >
<!ELEMENT limit	        							(#PCDATA)	                											   >
<!ATTLIST limit
	      unit	        (FC | FH | CY | LDG | AH | HRS | MO | DY | YE | YR 
							| A | C | D | AHR | DAY | MON | YRS | PF | T | AF 
							| DAILY | YES | MOS | CYC | CNG | IDGCNG | SHOPVST 
							| WHLCNG) 	#IMPLIED                                                                              >
<!ELEMENT skill         							(#PCDATA)                                                                                             >
<!ELEMENT feedback                                  (#PCDATA)                                                                 >
<!ELEMENT msno                                      (#PCDATA)                                                                 >
<!ELEMENT mshours                                   (#PCDATA)                                                                 >
<!ATTLIST mshours                                         
					man		CDATA                    #IMPLIED
					hours   CDATA                    #IMPLIED																  >
<!ELEMENT rii                                      	 (#PCDATA)                                                                >	
<!-- =====================================================================================================================  -->
<!--                              accomplished                                                                            	-->				
<!-- =====================================================================================================================  -->
<!ELEMENT accomplished (base,finding,deferredtask,manhours,worker,confirmer)           						                  >
<!ELEMENT base        								  (input) 	                                                          	  > 
<!ELEMENT finding        							  (input)            						                              >
<!ELEMENT deferredtask        						  (input)            						                              >
<!ELEMENT manhours                                    (input)                                                                >
<!ELEMENT worker  	 (workername, workerimage, workerdate)                                                                   >
<!ELEMENT confirmer  (workername, workerimage, workerdate)                       											 >
<!ELEMENT workername  	 (input)                                                                                  			 >
<!ELEMENT workerimage  	 (#PCDATA)                                                                              				 >
<!ELEMENT workerdate  	 (input)                                                                              				 >
<!ELEMENT panels     (pan*)                                                                                         		  >
<!ELEMENT sthours                                      (#PCDATA)                                                              >
<!ATTLIST sthours                                         
					man		CDATA                      #IMPLIED
					hours   CDATA                      #IMPLIED																  >
<!-- =====================================================================================================================  -->
<!-- =====================================================================================================================  -->
<!--     jobcard and input                                                                                                  -->
<!-- =====================================================================================================================  -->
<!ELEMENT input           EMPTY                               													              > 
<!ATTLIST input           id        		    ID           		#IMPLIED 
						  type	    (checkbox|radio|text)			#IMPLIED
						  value				    CDATA				#IMPLIED 												  >
<!-- ===================================================================================================================== -->
<!--     UNITS                                                                                                             -->
<!-- ===================================================================================================================== -->	
<!ELEMENT units           EMPTY                               													             > 
<!ATTLIST units          
						  val	 (deg.F|ft.|USgal|USquart|in.|in.2|lb|
									 lbf|lbf.in|lbf.ft|in.Hg|oz|psi|USgal.min|lb.min|deg.C|
									 m|l|mm|Mm2|Cm2|kg|daN|m.daN|hPa|g|bar|kg.min|
									 l.min|cc)			  					    #IMPLIED                                    >
<!-- ======================================================================================================================= -->
<!--     any where insert cqh-content                                                                                        -->
<!-- ========================================================================================================================--> 
<!ELEMENT cqh-content   	 (cb | con | csn | equ | grphcref
                         | %eins; | ncon | pan | refext | refint
                         | sub | super | ted | toc | txtgrphc
                         | std | zone| expd | stdname | tor 
						 | table | effect | para 
						 | list1 |l1item | list2 |l2item | list3 |l3item | list4 |l4item | list5 |l5item
						 | list6 |l6item | list7 |l7item | unlist | numlist 
						 | note | note-zh | warning | warning-zh | caution | caution-zh
						 | cqh-note | cqh-note-zh | cqh-warning| cqh-warning-zh | cqh-caution | cqh-caution-zh
						 | sign |  cqh-judge | cqh-condition | cqh-result
                         | refblock | toolnbr | refmedia 
                         | grsymbol | tadetail | riskdetail)+                                                       >	
<!ATTLIST 	cqh-content
							type  (TA|RISK|OTHER)	 	#IMPLIED												              >
<!-- ======================================================================================================================= -->
<!--     any where insert sign                                                                                               -->
<!-- ======================================================================================================================= -->
<!ELEMENT sign   	   	((mech?,insp?)| cb | con | csn | equ | grphcref
                         | (%eins;) | ncon | pan | refext | refint
                         | sub | super | ted | toc | txtgrphc
                         | std | zone| expd | stdname | tor 
						 | table | para 
						 | list1 | list2 | list3 | list4 | list5| list6 | list7 
						 | l1item | l2item | l3item | l4item | l5item | l6item | l7item
						 | unlist | numlist 
						 | note | note-zh | warning | warning-zh | caution | caution-zh
						 | cqh-note | cqh-note-zh | cqh-warning| cqh-warning-zh | cqh-caution | cqh-caution-zh
						 | sign | cqh-judge | cqh-condition | cqh-result
                         | refblock | toolnbr | refmedia 
                         | grsymbol | tadetail | cqh-content|subtask)+                                                 >
<!ATTLIST sign	
						id        					ID                            #IMPLIED								>	
<!ELEMENT mech		(mechname,mechdate) 																				>
<!ELEMENT insp		(inspname,inspdate)																				>
<!ATTLIST mech	
						authid 							CDATA		               #IMPLIED
						authname        				CDATA		               #IMPLIED
						ispreparation					(Y|N)		               #IMPLIED
						display							(Y|N)					   #IMPLIED
						checklevel						(RII|RCI)				   #IMPLIED
																								>
<!ELEMENT mechname		(input)			               											>
<!ELEMENT mechdate		(input)		               												>																																																										
<!ELEMENT inspdate		(input)		               												>																																																										
<!ATTLIST insp	
						authid         				CDATA		               #IMPLIED
						authname					CDATA					   #IMPLIED
						ispreparation				(Y|N)					   #IMPLIED
						display						(Y|N)					   #IMPLIED			>
<!ELEMENT inspname								(input)		               					>
<!ELEMENT tadetail  						    (#PCDATA) 		         	                    >
<!ATTLIST tadetail  						    
						tano 					 CDATA                        #IMPLIED					                     >
<!ELEMENT riskdetail  							(#PCDATA)  		                                                         >
<!ATTLIST riskdetail  						    
						riskno 					 CDATA                        #IMPLIED 	                                 >						
