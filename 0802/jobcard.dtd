<!-- ======================================================================================================================= -->
<!--     STANDARD LISTS                                                                                                      -->
<!-- ======================================================================================================================= -->
<!ELEMENT list1       (effect*,cqh-action?,cqh-judge?,cqh-condition?,cqh-result?, sign?,l1item*)                                                                           >
<!ELEMENT list2       (effect*,ltitle?,ltitle-zh?,cqh-action?,cqh-judge?,cqh-condition?,cqh-result?,sign? ,l2item*, table*)                                                          >
<!ELEMENT list3       (effect*,cqh-action?,cqh-judge?,cqh-condition?,cqh-result? , sign?,l3item*)                                                                           >
