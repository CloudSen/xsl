                <chgdesc>WARNING CAUTION ADDED, REVISED, OR DELETED</chgdesc>  
                <list1> 
                  <sign id="sign13504"> 
                    <mech authid="B201104" authname="A320-214机型（ME/AV）定检" ispreparation="Y"> 
                      <mechname> 
                        <input/> 
                      </mechname>  
